package me.struttle.plugins.events.commands;

import java.util.List;

public abstract class CommandBase {
	protected List<String> m_CommandDefinition;
	protected List<List<String>> m_Aliases;
	protected List<String> m_Permissions;
	protected String m_Definition;
	public boolean ExecuteCommand(String label, String[] args)
	{
		
	}
	
	public abstract boolean ExecuteInternal(String[] args, int index);
	
	public boolean Identify(String label, String[] args)
	{
		if(m_CommandDefinition.size() <= 1 + args.length)
			return false;
		if(!m_CommandDefinition.get(0).equalsIgnoreCase(label))
			return false;
	}
}

